# Environment Variables Template for SnapForge
# Copy this file to .env and fill in your actual values

# Stripe Configuration
# Note: STRIPE_SECRET_KEY is managed as a Firebase secret, not in .env
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here

# PayPal Configuration  
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_SECRET_KEY=your_paypal_secret_key_here
PAYPAL_SANDBOX_EMAIL=your_paypal_sandbox_email_here
PAYPAL_SANDBOX_PASSWORD=your_paypal_sandbox_password_here
PAYPAL_API_KEY=your_paypal_api_key_here
PAYPAL_API_SECRET=your_paypal_api_secret_here

# Replicate AI Configuration
REPLICATE_API_TOKEN=your_replicate_api_token_here
REPLICATE_BG_MODEL=your_replicate_background_removal_model_here

# Application Configuration
APP_NAME=SnapForge
